package app;

import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class DuplicateRemoverTest {
  @Test
  void removeReallyDuplication() {
    assertEquals("This is really stupid!!!", DuplicateRemoverOperation.convert("This is really really stupid!!!"));
  }

  @Test
  void returnTheSameSentence() {
    assertEquals("This is very really very cool", DuplicateRemoverOperation.convert("This is very really very cool"));
  }
}
